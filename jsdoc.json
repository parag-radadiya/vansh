{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["api"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "readme": "README.md"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}